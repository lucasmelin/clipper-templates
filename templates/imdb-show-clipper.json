{
  "schemaVersion": "0.1.0",
  "name": "IMDB show",
  "behavior": "append-specific",
  "noteContentFormat": "# {{selector:.hero__primary-text}}\n\n## Plot\n\n{{schema:description}}\n\n## Seasons Watched\n\n- [ ] Season 1\n- [ ] Season 2\n- [ ] Season 3\n\n## Notes\n\n",
  "properties": [
    {
      "name": "categories",
      "value": "[[Shows]]",
      "type": "multitext"
    },
    {
      "name": "genre",
      "value": "{{schema:genre|wikilink|join}}",
      "type": "multitext"
    },
    {
      "name": "imdbId",
      "value": "{{meta:property:imdb:pageConst}}",
      "type": "text"
    },
    {
      "name": "cover",
      "value": "{{image}}",
      "type": "text"
    },
    {
      "name": "creators",
      "value": "{{selector:[data-testid=\\\"title-pc-principal-credit\\\"]:nth-child(1) div a|unique|wikilink|join}}",
      "type": "multitext"
    },
    {
      "name": "cast",
      "value": "{{selector:[data-testid=\\\"title-pc-principal-credit\\\"]:nth-child(2) div a|unique|wikilink|join}}",
      "type": "multitext"
    },
    {
      "name": "year",
      "value": "{{schema:datePublished|split:\\\\\\\\\"-\\\\\\\\\"|slice:0,1}}",
      "type": "number"
    },
    {
      "name": "rating",
      "value": "",
      "type": "number"
    },
    {
      "name": "scoreImdb",
      "value": "{{schema:@TVSeries:aggregateRating.ratingValue}}",
      "type": "number"
    },
    {
      "name": "created",
      "value": "{{date}}",
      "type": "date"
    },
    {
      "name": "tags",
      "value": "shows, reviews, to-watch",
      "type": "multitext"
    },
    {
      "name": "plot",
      "value": "{{selector:span[data-testid=\\\"plot-xl\\\"]}}",
      "type": "text"
    },
    {
      "name": "seasons",
      "value": "{{selector:label[for=\\\"browse-episodes-season\\\"]}}",
      "type": "text"
    },
    {
      "name": "finale",
      "value": "true",
      "type": "checkbox"
    },
    {
      "name": "runtime",
      "value": "{{meta:property:og:description|split:\\\" | \\\"|first}}",
      "type": "text"
    },
    {
      "name": "contentRating",
      "value": "{{schema:@TVSeries:contentRating}}",
      "type": "text"
    },
    {
      "name": "title",
      "value": "{{selector:.hero__primary-text}}",
      "type": "text"
    }
  ],
  "triggers": [
    "schema:@TVShow",
    "/^https:\\/\\/www\\.imdb\\.com\\/title\\/tt\\d+\\/?\\S*$/"
  ],
  "noteNameFormat": "{{selector:.hero__primary-text}}",
  "path": "04 - Resources/Media"
}
